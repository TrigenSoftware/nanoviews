{
  "extends": [
    "@trigen/eslint-config/typescript"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "tsconfigRootDir": "./examples/weather-nanoviews",
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "import/no-absolute-path": "off",
    "import/no-default-export": "off",
    "import/no-anonymous-default-export": "off"
  }
}
